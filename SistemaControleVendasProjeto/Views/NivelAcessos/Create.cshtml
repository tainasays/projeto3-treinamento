@model SistemaControleVendasProjeto.Models.NivelAcesso

@{
    ViewData["Title"] = "Create";
}

<h1>Cadastrar</h1>

<h4>NivelAcesso</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TipoAcesso" class="control-label"></label>
                <input asp-for="TipoAcesso" class="form-control" />
                <span asp-validation-for="TipoAcesso" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Cadastrar" class="btn btn-info mb-2 mt-2" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Voltar para Lista</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
